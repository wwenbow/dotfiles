alias ls='ls -XF --color=auto --group-directories-first'
alias la='ls -A'
alias grep='grep --color=auto'
alias fgrep='fgrep --color=auto'
alias egrep='egrep --color=auto'
alias rm='rm -vfr'
alias cp='cp -ivR'
alias mv='mv -iv'
alias du='du -ch'
alias df='df -H'
alias bc='bc -l'
alias mkdir='mkdir -pv'
alias cd..='cd ..'
alias g='xdg-open'
alias ln='ln -v'
alias xclip="xclip -sel clip"
